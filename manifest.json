{
  "manifest_version": 3,
  "name": "AI DAO Manager",
  "version": "1.0",
  "description": "AI-powered DAO task management",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  }
} 