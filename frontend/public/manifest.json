{
  "manifest_version": 3,
  "name": "AIDAO Manager",
  "version": "1.0.0",
  "description": "Manage your tasks and track your progress",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "web_accessible_resources": [{
    "resources": [
      "tools_docs.json",
      "*.png"
    ],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_popup": "index.html"
  }
} 